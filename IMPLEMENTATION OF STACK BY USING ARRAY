#include <stdio.h>
#define MAX 5
int stack[MAX], top = -1;
void push(int x) {
 if (top == MAX - 1) {
 printf("Stack Overflow!\n");
 } else {
 top++;
 stack[top] = x;
 printf("%d pushed to stack\n", x);
 }
}
void pop() {
 if (top == -1) {
 printf("Stack Underflow!\n");
 } else {
 printf("%d popped from stack\n", stack[top]);
 top--;
 }
}
void display() {
 if (top == -1) {
 printf("Stack is empty\n");
 } else {
 printf("Stack elements: ");
 for (int i = 0; i <= top; i++) {
 printf("%d ", stack[i]);
 }
 printf("\n");
 }
}
int main() {
 push(10);
 push(20);
 display();
 pop();
 display();
 push(30);
 display();
 return 0;
}
