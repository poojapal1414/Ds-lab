#include <stdio.h>
int main() {
 int arr[5];
 int n,count=0 , loc, upd;
 printf(" enter the elements of array \n");
 for(int i=0; i<5; i++){
 scanf("%d",&arr[i]);
 }
 printf("enter the element you want to update\n");
 scanf("%d",&upd);
 printf("enter the location\n");
 scanf("%d",&loc);

 arr[loc-1] = upd;

 printf("the updated array is\n");
 for(int i=0; i<5; i++){
 printf("%d\t",arr[i]);
 }

 return 0;
}
