#include <stdio.h>
int main() {
 int arr[5] = {1,2,3,4,5};
 int n,count=0;
 printf("enter the elements to search\n");
 scanf("%d",&n);
 for(int i=0; i<5; i++){
 if(arr[i] == n){
 printf("%d is present in the %d position", n,i+1);
 count +=1;
 }
 }

 if (count ==0){
 printf("%d is not present in the array\n",n);
 }
 return 0;
}
